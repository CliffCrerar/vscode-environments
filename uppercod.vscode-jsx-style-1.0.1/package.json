{
	"name": "vscode-jsx-style",
	"displayName": "Vscode jsx style",
	"description": "Syntax highlighting for the style tag in jsx",
	"version": "1.0.1",
	"publisher": "uppercod",
	"engines": {
		"vscode": "^1.26.0"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/@atomico/vscode-jsx-style"
	},
	"categories": [
		"Programming Languages"
	],
	"contributes": {
		"languages": [
			{
				"id": "source.css.jsx.styled",
				"aliases": [
					"CSS (Styled JSX)"
				],
				"configuration": "./language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "source.css.jsx.styled",
				"scopeName": "source.css.jsx.styled",
				"path": "./syntaxes/css.json"
			},
			{
				"injectTo": [
					"source.js",
					"source.ts",
					"source.jsx",
					"source.js.jsx",
					"source.tsx"
				],
				"scopeName": "styled-jsx",
				"path": "./syntaxes/jsx-styled.json",
				"embeddedLanguages": {
					"source.css.scss": "css",
					"meta.embedded.line.ts": "typescript"
				}
			}
		]
	},
	"__metadata": {
		"id": "e59ee0a5-c994-4529-a670-216d2580d2b1",
		"publisherId": "7c28906e-2fb2-44e5-ab87-0adcedcd3000",
		"publisherDisplayName": "uppercod"
	}
}