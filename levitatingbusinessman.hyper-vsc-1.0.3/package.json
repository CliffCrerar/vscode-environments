{
	"name": "hyper-vsc",
	"displayName": "Hyper in VS Code",
	"description": "Add the Hyper terminal to VS Code's directory context menu",
	"version": "1.0.3",
	"publisher": "LevitatingBusinessMan",
	"icon": "images/Hyper.png",
	"repository": {
		"type": "git",
		"url": "https://github.com/LevitatingBusinessMan/hyper-vsc.git"
	},
	"engines": {
		"vscode": "^0.10.5"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.openHyper",
				"title": "Open Hyper here"
			}
		],
		"menus": {
			"explorer/context": [
				{
					"when": "explorerResourceIsFolder",
					"command": "extension.openHyper",
					"group": "navigation"
				}
			]
		}
	},
	"devDependencies": {
		"vscode": "^0.10.5"
	},
	"license": "MIT",
	"__metadata": {
		"id": "43ea9d9f-aa63-4673-a676-7ba53da909be",
		"publisherId": "02f43161-4e63-4351-a1ae-ab79991afcb9",
		"publisherDisplayName": "Rein Fernhout"
	}
}