{
	"name": "material-icons",
	"displayName": "Material Icons Library",
	"description": "Insert material icons straight into your code",
	"version": "0.2.0",
	"license": "MIT",
	"publisher": "Archety",
	"icon": "images/icon.png",
	"engines": {
		"vscode": "^1.20.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*",
		"onCommand:materialIcons.showLibrary"
	],
	"main": "./out/extension",
	"contributes": {
		"configuration": {
			"type": "object",
			"title": "MD Library configuration",
			"properties": {
				"material-icons.classList": {
					"type": "string",
					"default": "material-icon",
					"description": "Defines classes of newly inserted svgs"
				},
				"material-icons.includeXmlns": {
					"type": "boolean",
					"default": false,
					"description": "Choose whether the xmlns attribute should be included"
				},
				"material-icons.useFont": {
					"type": "boolean",
					"default": false,
					"description": "Use an icon font instead of the svgs"
				}
			}
		},
		"commands": [
			{
				"command": "materialIcons.showLibrary",
				"title": "Show Icons Library"
			}
		],
		"keybindings": [
			{
				"command": "materialIcons.showLibrary",
				"key": "ctrl+alt+i",
				"when": "editorTextFocus"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "tsc -p ./",
		"compile": "tsc -p ./",
		"watch": "tsc -w -p ./",
		"update-vscode": "node ./node_modules/vscode/bin/install",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"@types/mocha": "^2.2.42",
		"@types/node": "^7.0.63",
		"@types/request": "^2.47.0",
		"@types/yauzl": "^2.9.0",
		"eslint": "^4.11.0",
		"typescript": "^2.6.1",
		"vscode": "^1.1.17"
	},
	"dependencies": {
		"request": "^2.86.0",
		"yauzl": "^2.9.1"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/Archety/material-icons.git"
	},
	"homepage": "https://github.com/Archety/material-icons/blob/master/README.md",
	"__metadata": {
		"id": "f193b348-81d7-4b38-aab0-faef4a8fa3d1",
		"publisherId": "292e2797-3bd2-4569-8aaf-486dff341bc9",
		"publisherDisplayName": "Archety"
	}
}